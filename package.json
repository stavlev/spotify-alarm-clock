{
  "name": "spotify-library-tone",
  "version": "0.0.1",
  "description": "Analyze the tone of your Spotify library with IBM Watson",
  "main": "client/src/index.js",
  "scripts": {
    "start": "better-npm-run start",
    "build": "better-npm-run build",
    "dev": "better-npm-run dev",
    "lint": "eslint --ignore-path .gitignore ."
  },
  "betterScripts": {
    "build": {
      "command": "webpack --config webpack/prod.config.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "start": {
      "command": "node --harmony_destructuring_assignment server/app.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "node --harmony_destructuring_assignment server/app.js",
      "env": {
        "NODE_ENV": "development"
      }
    }
  },
  "dependencies": {
    "autoprefixer": "^6.3.0",
    "autoprefixer-loader": "^3.2.0",
    "babel": "^6.3.26",
    "babel-core": "^6.4.5",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.0",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "better-npm-run": "0.0.6",
    "bluebird": "^3.2.1",
    "body-parser": "^1.14.2",
    "clone": "^1.0.2",
    "cookie-parser": "^1.4.1",
    "css-loader": "^0.28.4",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^1.0.2",
    "eslint-plugin-react": "^3.16.1",
    "express": "~4.0.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^0.11.2",
    "history": "^1.17.0",
    "html-webpack-plugin": "^2.30.1",
    "morgan": "^1.6.1",
    "node-sass": "^4.7.2",
    "object-assign": "^4.0.1",
    "postcss-loader": "^2.0.6",
    "querystring": "^0.2.0",
    "raw-loader": "^0.5.1",
    "react": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-hot-loader": "^1.3.0",
    "react-redux": "^4.2.1",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "react-transform-catch-errors": "^1.0.1",
    "react-transform-hmr": "^1.0.2",
    "redbox-react": "^1.2.2",
    "redux": "^3.2.1",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-logger": "^2.4.0",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^6.0.6",
    "spotify-web-api-js": "^0.15.0",
    "spotify-web-api-node": "^2.2.0",
    "style-loader": "^0.18.2",
    "webpack": "^3.2.0",
    "webpack-dev-middleware": "^1.5.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.6.4"
  },
  "engines": {
    "node": ">5.0.0"
  }
}
